# Agentic Problem Solver Environment Configuration

# LLM Provider Configuration
LLM_PROVIDER=gemini
GEMINI_API_KEY=<REPLACE_WITH_YOUR_GEMINI_API_KEY>  # Get your API key from https://makersuite.google.com/app/apikey
GEMINI_MODEL=gemini-2.0-flash-lite
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_OUTPUT_TOKENS=2048
GEMINI_TOP_P=0.95
GEMINI_TOP_K=40

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=agent.log

# Agent Configuration
AGENT_MEMORY_PATH=./agent_memory
MAX_TOKENS_PER_REQUEST=8192
TEMPERATURE=0.7

# Vector Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=agent_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=<REPLACE_WITH_YOUR_DB_PASSWORD>

# Note: Copy this file to .env and replace the placeholder values with your actual API keys
# DO NOT commit your actual .env file to version control 